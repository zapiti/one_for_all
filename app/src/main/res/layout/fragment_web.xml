<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tool="http://schemas.android.com/tools"
    android:tag="webview"
   >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


<android.support.v7.widget.CardView
    android:id="@+id/infoLegendWeb"
    android:visibility="gone"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:elevation="15dp"
   >


    <RelativeLayout

        android:id="@+id/legend"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:background="@color/colorPrimary"
        android:paddingStart="10dp"
        android:paddingTop="10dp">

        <android.support.v7.widget.CardView
            android:id="@+id/web_view_frame1"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_alignParentTop="true"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="5dp"

            app:cardBackgroundColor="@color/noticiaChart"

            app:cardCornerRadius="10dp"

            />

        <android.support.v7.widget.CardView
            android:id="@+id/web_view_frame2"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_below="@+id/web_view_frame1"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="5dp"
            app:cardBackgroundColor="@color/entreterimentoChart"
            app:cardCornerRadius="10dp" />

        <android.support.v7.widget.CardView
            android:id="@+id/web_view_frame3"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_below="@+id/web_view_frame2"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="5dp"

            app:cardBackgroundColor="@color/saudeChart"
            app:cardCornerRadius="10dp" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_toEndOf="@+id/textView2">

            <android.support.v7.widget.CardView
                android:id="@+id/web_view_frame4"


                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_alignParentTop="true"
                android:layout_margin="5dp"

                app:cardBackgroundColor="@color/ongsChart"
                app:cardCornerRadius="10dp"

                />

            <android.support.v7.widget.CardView
                android:id="@+id/web_view_frame5"

                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_below="@+id/web_view_frame4"

                android:layout_margin="5dp"
                app:cardBackgroundColor="@color/ajudaChart"
                app:cardCornerRadius="10dp"

                />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_margin="5dp"

                android:layout_toEndOf="@+id/web_view_frame4"
                android:text="ONGs"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/web_view_frame4"
                android:layout_margin="5dp"
                android:layout_toEndOf="@+id/web_view_frame5"
                android:text="Ajuda"
                android:textSize="18sp" />
        </RelativeLayout>


        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="23dp"
            android:layout_margin="5dp"
            android:layout_toEndOf="@+id/web_view_frame1"
            android:text="Noticia"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/web_view_frame1"
            android:layout_margin="5dp"
            android:layout_marginEnd="10dp"
            android:layout_toEndOf="@+id/web_view_frame2"
            android:text="Entreterimento"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/web_view_frame2"
            android:layout_margin="5dp"

            android:layout_toEndOf="@+id/web_view_frame3"
            android:text="Saude"
            android:textSize="18sp" />


    </RelativeLayout>
</android.support.v7.widget.CardView>
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/infoButtonLegendy"
            android:text="kkk"
            android:layout_below="@+id/infoLegendWeb"/>

    <android.support.v7.widget.RecyclerView
        android:id="@+id/url_list_view"
        android:layout_below="@+id/infoLegendWeb"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    </RelativeLayout>
</FrameLayout>